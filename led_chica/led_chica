int LED_SOCKET = 5;
int interval = 500;

void setup() {
  // put your setup code here, to run once:
  pinMode(LED_SOCKET,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(LED_SOCKET, HIGH);
  delay(interval);
  digitalWrite(LED_SOCKET, LOW);
  delay(interval);
}
